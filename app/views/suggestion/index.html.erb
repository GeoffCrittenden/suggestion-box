<h1>Welcome to the Suggestion Box!</h1>
<p>Add a new suggestion below:</p>

<%= form_for :suggestion, url: suggestions_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
